{
	"env": {
		"browser": true,
		"es2023": true
	},
	"extends": ["standard-with-typescript", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": ["./tsconfig.json"]
	},
	"overrides": [],
	"rules": {
		"@typescript-eslint/restrict-template-expressions": "off"
	},
	"ignorePatterns": ["/src/env.d.ts", "/tsconfig.json", "/modules/**/*"]
}
